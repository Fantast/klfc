{
  "filter": "no tmk",
  "singletonKeys": [
    [ "Alt_R", "AltGr" ]
  ],
  "shiftlevels": [ "AltGr" ],
  "keys": [
    { "pos": "Q", "letters": [ "^" ] },
    { "pos": "W", "letters": [ "&" ] },
    { "pos": "E", "letters": [ "[" ] },
    { "pos": "R", "letters": [ "]" ] },

    { "pos": "A", "letters": [ "|" ] },
    { "pos": "S", "letters": [ "+" ] },
    { "pos": "D", "letters": [ "(" ] },
    { "pos": "F", "letters": [ ")" ] },
    { "pos": "G", "letters": [ "=" ] },

    { "pos": "Z", "letters": [ "\\" ] },
    { "pos": "X", "letters": [ "*" ] },
    { "pos": "C", "letters": [ "{" ] },
    { "pos": "V", "letters": [ "}" ] },

    { "pos": "O", "letters": [ "Alt+Up" ] },
    { "pos": "K", "letters": [ "Alt+Left" ] },
    { "pos": "L", "letters": [ "Alt+Down" ] },
    { "pos": ";", "letters": [ "Alt+Right" ] },

    { "pos": "Up", "letters": [ "Alt+Up" ] },
    { "pos": "Left", "letters": [ "Alt+Left" ] },
    { "pos": "Down", "letters": [ "Alt+Down" ] },
    { "pos": "Right", "letters": [ "Alt+Right" ] },

    { "pos": "Tab", "letters": [ "Tab" ] },
    { "pos": "space", "letters": [ " " ] }
  ]
}
