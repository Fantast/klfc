{
  "filter": "no tmk",
  "singletonKeys": [
    [ "Alt_R", "AltGr" ]
  ],
  "shiftlevels": [ "AltGr" ],
  "keys": [
    { "pos": "Q", "letters": [ "^" ] },
    { "pos": "W", "letters": [ "&" ] },
    { "pos": "E", "letters": [ "[" ] },
    { "pos": "R", "letters": [ "]" ] },

    { "pos": "A", "letters": [ "|" ] },
    { "pos": "S", "letters": [ "+" ] },
    { "pos": "D", "letters": [ "(" ] },
    { "pos": "F", "letters": [ ")" ] },
    { "pos": "G", "letters": [ "=" ] },

    { "pos": "X", "letters": [ "*" ] },
    { "pos": "C", "letters": [ "{" ] },
    { "pos": "V", "letters": [ "}" ] },

    { "pos": "space", "letters": [ " " ] }
  ]
}
