{
  "filter": "no klc,keylayout",
  "singletonKeys": [
    [ "CapsLock", "Extend" ]
  ],
  "shiftlevels": [ "Extend", "Shift+Extend" ],
  "keys": [
    { "pos": "O", "letters": [ "Up" ] },
    { "pos": "K", "letters": [ "Left" ] },
    { "pos": "L", "letters": [ "Down" ] },
    { "pos": ";", "letters": [ "Right" ] },

    { "pos": "I", "letters": [ "Control+Left" ] },
    { "pos": "P", "letters": [ "Control+Right" ] },
    { "pos": "J", "letters": [ "Home" ] },
    { "pos": "'", "letters": [ "End" ] },
    { "pos": "U", "letters": [ "PageUp" ] },
    { "pos": "[", "letters": [ "PageDown" ] },
    { "pos": "]", "letters": [ "Insert" ] },

    { "pos": "Backspace", "letters": [ "Delete" ] },

    { "pos": "M", "letters": [ "Control+Tab" ] },

    { "pos": "Space", "letters": [ "Shift" ] }
  ]
}
