{
  "filter": "no tmk",
  "singletonKeys": [
    [ "Alt_R", "AltGr" ]
  ],
  "shiftlevels": [ "AltGr" ],
  "keys": [
    { "pos": "Q", "letters": [ "^" ] },
    { "pos": "W", "letters": [ "&" ] },
    { "pos": "E", "letters": [ "[" ] },
    { "pos": "R", "letters": [ "]" ] },

    { "pos": "A", "letters": [ "|" ] },
    { "pos": "S", "letters": [ "+" ] },
    { "pos": "D", "letters": [ "(" ] },
    { "pos": "F", "letters": [ ")" ] },
    { "pos": "G", "letters": [ "=" ] },

    { "pos": "Z", "letters": [ "\\" ] },
    { "pos": "X", "letters": [ "*" ] },
    { "pos": "C", "letters": [ "{" ] },
    { "pos": "V", "letters": [ "}" ] },

    { "pos": "I", "letters": [ "Alt+Up" ] },
    { "pos": "J", "letters": [ "Alt+Left" ] },
    { "pos": "K", "letters": [ "Alt+Down" ] },
    { "pos": "L", "letters": [ "Alt+Right" ] },

    { "pos": "Y", "letters": [ "red:Alt+Y" ] },
    { "pos": "U", "letters": [ "red:Alt+U" ] },
    { "pos": "O", "letters": [ "red:Alt+O" ] },
    { "pos": "P", "letters": [ "red:Alt+P" ] },
    { "pos": "[", "letters": [ "red:Alt+/" ] },
    { "pos": "H", "letters": [ "red:Alt+H" ] },
    { "pos": ";", "letters": [ "red:Alt+;" ] },
    { "pos": "'", "letters": [ "red:Alt+'" ] },
    { "pos": "N", "letters": [ "red:Alt+N" ] },
    { "pos": "M", "letters": [ "red:Alt+M" ] },
    { "pos": ",", "letters": [ "red:Alt+," ] },
    { "pos": ".", "letters": [ "red:Alt+." ] },

    { "pos": "Tab", "letters": [ "Tab" ] },
    { "pos": "Space", "letters": [ "SwitchLayout" ] },
    { "pos": "Enter", "letters": [ "red:Alt+Enter" ] },
    { "pos": "Backspace", "letters": [ "red:Alt+Backspace" ] }
  ]
}
